@model Service
@{
    ViewData["Title"] = "Create";
}
<div class="container">
    <div class="col-md-10">
        <h1>Create new service</h1>
        <form asp-action="create" method="post" enctype="multipart/form-data">

    <div class="form-group">
        <label asp-for="Image">Service Image</label>
        <input asp-for="Image" type="file" class="form-control" placeholder="Image">
    </div>
    <div class="form-group">
        <label asp-for="Title">Service title</label>
        <input asp-for="Title" type="text" class="form-control" placeholder="Enter title">
    </div>
    <div class="form-group">
        <label asp-for="Description">Service description</label>
        <input asp-for="Description" type="text" class="form-control" placeholder="Enter description">
    </div>
    <div class="form-group">
        <label asp-for="IconUrl">Icon url</label>
        <input asp-for="IconUrl" type="text" class="form-control" placeholder="Enter url">
    </div>
    


  <button type="submit" class="btn btn-primary">Submit</button>
</form>
    </div>
</div>


